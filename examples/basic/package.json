{
  "name": "plotly",
  "version": "1.0.0",
  "scripts": {
    "start": "idyll index.idl --css styles.css --layout blog --spellcheck",
    "build": "idyll index.idl --layout blog --css styles.css --build | uglifyjs > build/index.js; cp .idyll/styles.css build/",
    "deploy": "npm run build && gh-pages -d ./build"
  },
  "devDependencies": {
    "gh-pages": "^0.12.0",
    "idyll-component": "^1.0.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "uglify-js": "^2.8.22",
    "uglifyjs": "^2.4.10"
  },
  "dependencies": {
    "idyll": "^1.0.25"
  }
}
